#define _CRT_SECURE_NO_WARNINGS 1

#include<stdio.h>
#include<string.h>
#include<windows.h>
#include<stdlib.h>

选择语句的使用
int main()
{
	int a = 1;
	int b = 0;
	while (a <= 100)
	{
		b = a % 2;
		if (1==b)
		{
			printf("%d ", a);
		}

		a+=1;
	}

	return 0;
}

int main()//
{
	int a =0;
	printf("你的性别？ \n");
	printf("男（0）  女（1）  保密（2）\n");
	scanf("%d", &a);
	if (a == 0)
		printf("男\n");
	else if (a == 1)
		printf("女\n");
	else
		printf("保密\n");
	return 0;
}
